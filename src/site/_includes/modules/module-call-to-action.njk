<section class="section section--module section--module-cta">
	{% include 'components/module-base.njk' %}
	<div class="section__module">
		{% for cta in module.ctas %}
			<div class="cta">
				<a class="button button--cta" href="{{ cta.href }}">
					{{ cta.text }}
				</a>
			</div>	
		{% endfor %}
	</div>
</section>
